	<ListOfMetaMeme>
		
		<!-- Agent -->
		<MetaMeme id="OrderedListMember">
			<MetaMemeProperty name="Priority" type="integer"/>
		</MetaMeme>
		<MetaMeme id="Taxonomy"/>
		<MetaMeme id="BubbleUp">
			<MetaMemeProperty name="Condition" type="string"/>
			<MetaMemeProperty name="MergeScript" type="string"/>
			<MetaMemeProperty name="Property" type="string"/>
			<MetaMemeProperty name="Restrictive" type="boolean"/>
			<MemberMetaMeme reference="MergeScript" min="0" max="1"/>
		</MetaMeme>	
		<MetaMeme id="MergeScript">
			<MemberMetaMeme reference="" min="1" max="1"/>
		</MetaMeme>
		<MetaMeme id="BubbleUpParadigm">
			<MetaMemeProperty name="Tiered" type="boolean"/>
			<MetaMemeProperty name="Restrictive" type="boolean"/>
			<MemberMetaMeme reference="BubbleUp" min="1"/>
		</MetaMeme>	

		<MetaMeme id="Thing">
			<MetaMemeProperty name="Tags" type="list"/>
			<MemberMetaMeme reference="Taxonomy" min="0"/>
			<MemberMetaMeme reference="Event" min="0" max="1"/>
			<MemberMetaMeme reference="BubbleUpParadigm" min="0" max="1"/>
		</MetaMeme>	

		<!-- Condition -->

		<MetaMeme id="ConditionalMember">
			<MetaMemeExtensions>
				<MetaMemeID>OrderedListMember</MetaMemeID>	
			</MetaMemeExtensions>
			<MemberMetaMeme reference="graphyne.Condition.Condition" min="1" max="1"/>
		</MetaMeme>
		
		<!-- Controller -->
		<MetaMeme id="Whitelist">
			<MetaMemeProperty name="Action" type="list"/>
			<MetaMemeProperty name="Stimulus" type="list"/>
		</MetaMeme>	
		<MetaMeme id="Blacklist">
			<MetaMemeProperty name="Action" type="list"/>
			<MetaMemeProperty name="Stimulus" type="list"/>
		</MetaMeme>
		<MetaMeme id="ControllerDeclaration" switch="true">
			<MetaMemeProperty name="SubscribedDescriptors" type="list"/>
			<MetaMemeProperty name="SubscribedChannels" type="list"/>
			<MemberMetaMeme reference="Whitelist"/>
			<MemberMetaMeme reference="Blacklist"/>
		</MetaMeme>

		<!-- Scripts -->
		<MetaMeme id="StateEventScript" singleton="true">
			<MetaMemeProperty name="State" type="string" restriction="StateEventType" constrained="true"/>
			<MetaMemeProperty name="PropertyID" type="string"/>
			<MemberMetaMeme reference="Script" min="1" max="1"/>
		</MetaMeme>		
		<MetaMeme id="Script" singleton="true">
			<MetaMemeProperty name="Script" type="string"/>
			<MetaMemeProperty name="Language" type="string" constrained="true" restriction="ScriptLanguage"/>
		</MetaMeme>
		
		<!-- Channel -->
		<MetaMeme id="ChannelDeclaration">
			<MetaMemeProperty name="EndorsedControllerTemplates" type="list"/>
			<MetaMemeProperty name="DistributionParadigm" type="string"/>
			<MetaMemeProperty name="DescriptorType" type="string"/>
			<MetaMemeProperty name="Description" type="string"/>
		</MetaMeme>	
		

	</ListOfMetaMeme>
	
	<ListOfMeme>
		<Meme id="TrueScript" metameme="Script">
			<MemeProperty name="Script" value="graphyne.Scripts.ConditionTrue"/>
			<MemeProperty name="Language" value="python"/>
		</Meme>
		<Meme id="TrueSES" metameme="StateEventScript">
			<MemeProperty name="State" value="execute"/>
			<MemberMeme occurrence="1" memberID="TrueScript" />
		</Meme>	

		<Meme id="FalseScript" metameme="Script">
			<MemeProperty name="Script" value="graphyne.Scripts.ConditionFalse"/>
			<MemeProperty name="Language" value="python"/>
		</Meme>
		<Meme id="FalseSES" metameme="StateEventScript">
			<MemeProperty name="State" value="execute"/>
			<MemberMeme occurrence="1" memberID="FalseScript" />
		</Meme>		

	</ListOfMeme>

